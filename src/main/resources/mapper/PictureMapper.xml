<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mybatisplus.mapper.PictureMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.mybatisplus.model.domain.Picture">
        <id column="id" property="id" />
        <result column="user_id" property="userId" />
        <result column="url" property="url" />
        <result column="title" property="title" />
    </resultMap>
    <select id="getRank" resultMap="BaseResultMap">
        select * from picture where user_id = #{userId}
    </select>
    <select id="getMyPic" resultMap="BaseResultMap">
        select * from picture where user_id = #{userId}
    </select>

</mapper>
